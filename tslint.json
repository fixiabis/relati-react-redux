{
    "defaultSeverity": "warn",
    "extends": [
        "tslint-react"
    ],
    "jsRules": {
        "eofline": true
    },
    "rules": {
        "member-access": false,
        "ordered-imports": false,
        "quotemark": false,
        "no-console": false,
        "semicolon": false,
        "jsx-no-lambda": false,
        "jsx-boolean-value": false,
        "no-switch-case-fall-through": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts"
        ]
    }
}